@model GestionInventarioWeb.Data.User;
@{
    Layout = "_Layout";
    ViewData["Title"] = "Dashboard";
    ViewData["model"] = Model;
}

<div class="container-fluid bg-white mb-3">
    <h1>Ventas</h1>
    @if(Model == null)
    {
        <div class="alert alert-warning">Error al obtener datos!</div>
    }
    else
    {
        <p>Hola @Model.Name</p>
        <p>Tu rut es @Model.Rut</p>
    }
</div>
